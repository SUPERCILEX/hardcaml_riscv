{
  "emeraldwalk.runonsave": {
    "commands": [
      {
        "match": "\\.ml$",
        "cmd": "dune runtest --auto-promote"
      },
      {
        "match": "\\.ml$",
        "cmd": "dune build @fmt --auto-promote"
      },
      {
        "match": "\\.ml$",
        "cmd": "dune build --auto-promote"
      },
      {
        "match": "\\.ml$",
        "cmd": "./arty/arty.exe compile -output arty/top.v"
      },
    ]
  }
}
